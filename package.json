{
  "name": "opo-scraper",
  "version": "1.0.0",
  "description": "Multi-source scraper for US Organ Procurement Organizations (OPOs)",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "scrape": "node src/index.js",
    "scrape:opodata": "node src/index.js --source=opodata",
    "scrape:propublica": "node src/index.js --source=propublica",
    "scrape:hrsa": "node src/index.js --source=hrsa",
    "scrape:srtr": "node src/index.js --source=srtr",
    "scrape:cms-qcor": "node src/index.js --source=cms-qcor",
    "normalize": "node src/index.js --normalize-only"
  },
  "dependencies": {
    "axios": "^1.7.0",
    "cheerio": "^1.0.0",
    "puppeteer": "^23.0.0",
    "xlsx": "^0.18.5"
  },
  "engines": {
    "node": ">=18"
  }
}
